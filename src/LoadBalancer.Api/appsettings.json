{
  "LoadBalancer": {
    "ListenPort": 6000,
    "Backends": [
      "tcp://127.0.0.1:5001",
      "tcp://127.0.0.1:5002",
      "tcp://127.0.0.1:5003",
      "tcp://127.0.0.1:5004",
      "tcp://127.0.0.1:5005"
    ],
    "HealthCheckIntervalSeconds": 5,
    "Strategy": "RoundRobin"
  },
  "TcpForwarder": {
    "MaxConcurrentConnections": 100,
    "IdleTimeoutSeconds": 15,
    "MaxLifetimeSeconds": 300,
    "BufferSize": 8192
  },
  "Serilog": {
    "MinimumLevel": "Information"
  }
}